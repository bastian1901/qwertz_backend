const { getAblauf } = require("./ablaufController");
const { getKonversation } = require("./konversationController");
const { getWimmelbild } = require("./wimmelbildController");
const { getZuordnung } = require("./zuordnungController");

const getModi = ((req, res) => {
    switch (req.params.modiname) {
        case "Ablaufanordnung":
            getAblauf(req,res)
            break;
        case "Konversation":
            getKonversation()
            break;
        case "Wimmelbild":
            getWimmelbild()
            break;
        case "Zuordnung":
            getZuordnung()
            break;
    }

})

module.exports = {
    getModi
}
